{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cp src/assets/copied/bootstrap.css node_modules/bootstrap/dist/css/bootstrap.css && ng serve --disable-host-check",
    "build": "cp src/assets/copied/bootstrap.css node_modules/bootstrap/dist/css/bootstrap.css && ng build && gulp add-querystring-to-index && ng run client:server && webpack --config webpack.server.config.js --progress --colors",
    "server": "node dist/server",
    "build-prod": "cp src/assets/copied/bootstrap.css node_modules/bootstrap/dist/css/bootstrap.css && ng build --prod && gulp add-querystring-to-index && ng run client:server:production && webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run client:server:production",
    "format:check": "prettier --config .prettierrc.json --list-different \"src/{app,environments}/**/*{.ts,.js,.json,.css,.scss}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/platform-server": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@nguniversal/express-engine": "7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "add": "^2.0.6",
    "angular-markdown-editor": "^2.0.2",
    "angular2-image-upload": "^1.0.0-rc.1",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "ng2-adsense": "^5.4.1",
    "ngx-markdown": "^7.1.3",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "yarn": "^1.12.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "gulp": "^4.0.0",
    "gulp-replace": "^1.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.17.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-cli": "^3.1.0"
  }
}
